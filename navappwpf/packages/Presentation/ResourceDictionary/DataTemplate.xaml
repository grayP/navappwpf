<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:navappwpf.ViewModels"
                    xmlns:vw="clr-namespace:navappwpf.Views">
    <DataTemplate DataType="{x:Type vm:ReadingsViewModel}">
        <vw:NavigationReading />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DirSpeedViewModel}">
        <vw:DistAndSpeed />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WindViewModel}">
        <vw:Wind />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <vw:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TrendViewModel}">
        <vw:Trend />
    </DataTemplate>
</ResourceDictionary>